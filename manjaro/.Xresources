!!! URxvt configuration
!! Fonts
URxvt.font: 		xft:Anonymous Pro:size=14
URxvt.boldFont: 	xft:Anonymous Pro:size=14
URxvt.italicFont: 	xft:Anonymous Pro:size=14
URxvt.boldItalicfont: 	xft:Anonymous Pro:size=14

!! Indentation
URxvt.letterSpace:	-5
URxvt.lineSpace: 	0
URxvt.internalBorder: 	0

!! Behavior
URxvt.tabbedex.autohide: 	false
URxvt.cursorUnderline: 		true
URxvt.saveline: 		2048
URxvt.scrollBar: 		false
URxvt.urgentOnBell: 		true
URxvt*scrollstyle: 		plain
URxvt.url-launcher: 		firefox
URxvt.matcher.button: 		1
URxvt*background: 		#0A090A
URxvt*depth: 			32
URxvt.iso14755:			false
URxvt.iso14755_52:		false

!! Extensions
URxvt.perl-ext-common:default,clipboard,keyboard-select,tabbedex,matcher

!! Key bindings
URxvt.keysym.Shift-Control-C: 			perl:clipboard:copy
URxvt.keysym.Shift-Control-V: 			perl:clipboard:paste
URxvt.keysym.Control-t: 			perl:tabbedex:new_tab
URxvt.keysym.Control-Tab: 			perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-ISO_Left_Tab: 	perl:tabbedex:prev_tab

